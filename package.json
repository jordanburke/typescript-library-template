{
  "name": "-template",
  "version": "8.0.0",
  "description": "A basic template for creating a TypeScript library and Publishing it to NPM.",
  "keywords": [
    "typescript",
    "template",
    "tsdown",
    "npm"
  ],
  "author": "jordan.burke@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/jordanburke/typescript-library-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanburke/typescript-library-template"
  },
  "scripts": {
    "validate": "ts-builds validate",
    "format": "ts-builds format",
    "format:check": "ts-builds format:check",
    "lint": "ts-builds lint",
    "lint:check": "ts-builds lint:check",
    "typecheck": "ts-builds typecheck",
    "test": "ts-builds test",
    "test:watch": "ts-builds test:watch",
    "test:coverage": "ts-builds test:coverage",
    "build": "ts-builds build",
    "dev": "ts-builds dev",
    "prepublishOnly": "pnpm validate"
  },
  "devDependencies": {
    "@types/node": "^24.10.10",
    "ts-builds": "^2.3.2",
    "tsdown": "^0.18.4"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "lib",
    "dist"
  ],
  "prettier": "ts-builds/prettier",
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
